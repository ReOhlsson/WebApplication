<div class="number">
    <img class="channelIcon" src=@Model.ChannelIconUrl />
    <h2 class="show-heading">@Html.ActionLink((string)@Model.ChannelName , "Index", "Channel", new { channel = Model.ChannelOriginalName }, null)</h2>  
    <ul class="program">
        @foreach (var item in Model.ProgramJsonList)
        {
            <li><a class="@("programLink" + @item.HasPassed)" onclick="showPopUp(this)" id="@item.title.sv" data-title="@item.title.sv" data-start="@item.start" data-stop="@item.stop" data-desc="@item.desc.sv" data-channel="@item.channel" data-category="@item.CategoryToString"data-timeStart="@item.StartTime.ToShortTimeString()" data-timeEnd="@item.StopTime.ToShortTimeString()">@item.StartTime.ToShortTimeString()  @item.title.sv</a></li>
        }
   </ul>
</div>

